<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="starting.StartingController">
   <bottom>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="293.0" prefWidth="211.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="m_NewRepositoryBtn" mnemonicParsing="false" onAction="#CreateNewRepositry_OnClick" prefHeight="65.0" prefWidth="211.0" text="New Repository" GridPane.rowIndex="1" />
            <Button fx:id="m_LoadRepoFromXMLBtn" mnemonicParsing="false" onAction="#LoadRepositoryFromXML_OnClick" prefHeight="390.0" prefWidth="1245.0" text="Load Repository From XML" GridPane.rowIndex="2" />
            <Button fx:id="m_LoadExistingRepositoryBtn" mnemonicParsing="false" onAction="#LoadExistingRepository_OnClick" prefHeight="193.0" prefWidth="677.0" text="Load Existing Repositoty" GridPane.rowIndex="3" />
            <Button fx:id="m_CloneBtn" mnemonicParsing="false" onAction="#Clone_OnClick" prefHeight="424.0" prefWidth="1147.0" text="Clone Repository" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </bottom>
   <center>
      <Label fx:id="m_WelcomeLabel" alignment="CENTER" prefHeight="178.0" prefWidth="420.0" text="Welcome to M.A.Git" textAlignment="CENTER" textFill="#300bd3" textOverrun="CLIP" BorderPane.alignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
   </center>
   <right>
      <ProgressBar fx:id="m_ProgressBar" prefHeight="23.0" prefWidth="225.0" progress="0.0" visible="false" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
